Algoritmo "QUESTAO10"


Var
       NumA, NumB: real

Inicio
      escreval ("Digite o primeiro número")
      leia (NumA)
      escreval ("O primeiro número foi: ", NumA)
      escreval ("Digite o segundo número")
      leia(NumB)
      escreval ("O segundo número foi: ", NumB)
      escreval ("A ordem inversa é: ", NumB ,",", NumA)


Fimalgoritmo