Algoritmo "QUESTAO6"


Var

       velocidade, metros, tempo: real
       
Inicio
       escreval ("Informe o velocidade, em km/h")
       leia (velocidade)
       tempo <- 434.4 / velocidade
       metros <- velocidade / 3.6
       escreval ("A velocidade em m/s: ", metros:2:3)
       escreval ("O tempo de viagem de São Paulo até o Rio de Janeiro será: ", tempo:1:0, "hrs")

Fimalgoritmo