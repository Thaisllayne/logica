Algoritmo "QUESTAO11"


Var
     n1, n2, n3, p1, p2, p3, media: real

Inicio
     p1 <- 2
     p2 <- 3
     p3 <- 5
     escreval ("Digite a primeira nota")
     leia (n1)
     escreval ("Digite a segunda nota")
     leia (n2)
     escreval ("Digite a terceira nota")
     leia (n3)
     
     media <- ((n1*p1)+(n2*p2)+(n3*p3))/(p1+p2+p3)
     escreval ("A média final foi: ", media)


Fimalgoritmo