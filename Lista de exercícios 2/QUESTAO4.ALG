algoritmo "QUESTAO4"
var
     valorfixado, valorvendas : real


inicio
       escreval ("Digite o valor fixo")
       leia (valorfixado)
       escreval ("Digite o valor de vendas do funcionário")
       leia (valorvendas)
       se valorvendas >= valorfixado entao
          escreval ("Ganhou!")
       senao
          escreval ("Não Ganhou!")
       fimse


fimalgoritmo